#1
library("spatstat")
data(murchison)
par(mfrow = c(1, 1))
v.gold = murchison$gold
v.faults = murchison$faults
v.greenstone = murchison$greenstone
plot(murchison$faults, col = 'red', main = "Złoto i uskoki")
plot(murchison$gold, add = TRUE, col = 'gold')
class(v.gold)
class(v.faults)
#2
distance_map <- distmap(v.faults)
plot(distance_map, main = "Mapa Z(u): Odległość od najbliższego uskoku")
plot(v.gold, add = TRUE, col = 'gold')
#4
Z_u <- distfun(v.faults)
rho_estimation <- rhohat(v.gold, Z_u)
plot(rho_estimation, main = "Intensywność złóż złota zależna od odległości od uskoków",
xlab = "Odległość od najbliższego uskoku", ylab = "Intensywność", col = "blue", lwd = 2)
#5
distance_function <- distfun(v.faults)
distance_area <- as.im(distance_function)
plot(distance_area, main = "Odległość od najbliższego uskoku")
plot(v.gold, add = TRUE, col = 'gold')
#6
distance_function(400000, 7000000)
#7
par(mfrow = c(1, 2))
plot(distance_map, main = "Mapa odległości ćw 2")
plot(v.gold, add = TRUE, col = 'gold')
plot(distance_area, main = "Mapa odległości ćw 5")
plot(v.gold, add = TRUE, col = 'gold')
